<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="198.0" prefWidth="443.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/25" fx:controller="camapps.stackers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Welcome to">
      <font>
         <Font size="21.0" />
      </font>
   </Label>

    <Label fx:id="welcomeText" text="STACKERS">
      <font>
         <Font name="Helvetica Neue Bold" size="37.0" />
      </font></Label>
   <Label text="Press &quot;Play&quot; to continue!" />
    <Button minHeight="25.0" minWidth="100.0" onAction="#onHelloButtonClick" text="Play" />
</VBox>
